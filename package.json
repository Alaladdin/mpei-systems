{
  "name": "mpei-systems",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",

    "docker:build": "docker build -t mpei-webhook .",
    "docker:run": "docker run -d -p 9001:9001 --name mpei-webhook --env-file .env -v %cd%/docker:/usr/src/app/docker mpei-webhook",
    "docker:clean": "docker container rm mpei-webhook --volumes -f && docker image rm mpei-webhook -f",
    "docker:recreate": "npm run docker:clean && npm run docker:build && npm run docker:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alaladdin/mpei-systems.git"
  },
  "keywords": ["docker"],
  "author": "Alaladdin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alaladdin/mpei-systems/issues"
  },
  "homepage": "https://github.com/Alaladdin/mpei-systems#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "koa": "^2.13.4",
    "koa-body-parser": "^1.1.2",
    "koa-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19"
  }
}
